{
    "name": "bakerkretzmar/laravel-mapbox",
    "description": "A Laravel package for managing Mapbox Datasets and Tilesets.",
    "keywords": ["laravel", "mapbox"],
    "homepage": "https://github.com/bakerkretzmar/laravel-mapbox",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "orchestra/testbench": "^7.19",
        "phpunit/phpunit": "^9.5.10"
    },
    "authors": [
        {
            "name": "Jacob Baker-Kretzmar",
            "email": "jacobtbk@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bakerkretzmar\\LaravelMapbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bakerkretzmar\\LaravelMapbox\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bakerkretzmar\\LaravelMapbox\\LaravelMapboxServiceProvider"
            ],
            "aliases": {
                "Mapbox": "Bakerkretzmar\\LaravelMapbox\\Facades\\Mapbox"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
